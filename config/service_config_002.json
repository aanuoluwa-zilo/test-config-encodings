{
  "service": {
    "name": "microservice-2",
    "version": "2.2.1",
    "environment": "production",
    "port": 3002,
    "host": "0.0.0.0"
  },
  "database": {
    "host": "db-cluster-2.example.com",
    "port": 5432,
    "name": "service_db_2",
    "username": "service_user_2",
    "password": "${DB_PASSWORD}",
    "ssl": {
      "enabled": true,
      "mode": "require"
    },
    "pool": {
      "min": 5,
      "max": 50,
      "acquire": 30000,
      "idle": 10000
    }
  },
  "cache": {
    "redis": {
      "host": "redis-cluster-2.example.com",
      "port": 6379,
      "password": "${REDIS_PASSWORD}",
      "db": 0,
      "maxConnections": 100,
      "retryDelayOnFailover": 100
    }
  },
  "logging": {
    "level": "info",
    "format": "json",
    "output": "stdout",
    "timestamp": true,
    "correlationId": true
  },
  "security": {
    "jwt": {
      "secret": "${JWT_SECRET}",
      "expiration": 3600,
      "refreshExpiration": 604800
    },
    "cors": {
      "origins": [
        "https://app2.example.com",
        "https://admin2.example.com"
      ],
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": ["Content-Type", "Authorization"]
    },
    "rateLimit": {
      "windowMs": 900000,
      "maxRequests": 1000,
      "skipSuccessfulRequests": false
    }
  },
  "external": {
    "payment": {
      "provider": "stripe",
      "apiKey": "${STRIPE_API_KEY}",
      "webhookSecret": "${STRIPE_WEBHOOK_SECRET}"
    },
    "email": {
      "provider": "sendgrid",
      "apiKey": "${SENDGRID_API_KEY}",
      "fromEmail": "noreply@service2.com"
    },
    "storage": {
      "provider": "aws",
      "bucket": "service-files-2",
      "region": "us-east-1",
      "accessKey": "${AWS_ACCESS_KEY}",
      "secretKey": "${AWS_SECRET_KEY}"
    }
  },
  "monitoring": {
    "metrics": {
      "enabled": true,
      "interval": 60,
      "endpoint": "/metrics"
    },
    "health": {
      "enabled": true,
      "endpoint": "/health",
      "timeout": 5000
    },
    "tracing": {
      "enabled": true,
      "sampling": 0.1
    }
  },
  "features": {
    "newUI": true,
    "advancedAnalytics": false,
    "betaFeatures": true,
    "maintenanceMode": false
  }
}